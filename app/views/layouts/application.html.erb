<!DOCTYPE html>
<html>
<head>
  <title>OttawaSchools</title>
  <%= stylesheet_link_tag 'global.css' %>
  <%= stylesheet_link_tag 'simple_form.css' %>  
  <%= stylesheet_link_tag 'http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.6/themes/smoothness/jquery-ui.css' %>      
  <%= yield :stylesheet_includes %>
  
  <%= javascript_include_tag "https://ajax.googleapis.com/ajax/libs/jquery/1.4.3/jquery.min.js" %>
  <%= javascript_include_tag "https://ajax.googleapis.com/ajax/libs/jqueryui/1.8.6/jquery-ui.js" %>
  <%= javascript_include_tag "application.js" %>  
  <%= javascript_include_tag "home.js" %>  
  <%= yield :javascript_includes %>
  
  <%= csrf_meta_tag %>

  <script type="text/javascript">

    var _gaq = _gaq || [];
    _gaq.push(['_setAccount', 'UA-20316714-1']);
    _gaq.push(['_trackPageview']);

    (function() {
      var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
      ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
      var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
    })();

  </script>
</head>
<body>

<%= yield %>

</body>
</html>
